<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <Version>2.1.6</Version>
    <Authors>Intent Architect</Authors>
    <Description>Attributes for Intent Architect C# code weaver.</Description>
    <Copyright>Copyright 2022</Copyright>
    <PackageProjectUrl>https://intentarchitect.com/</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/IntentSoftware/Intent.RoslynWeaver.Attributes</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>